<openerp>
    <data>
        <record id="tk_cron_ir_cron_view_tree_inherited" model="ir.ui.view">
            <field name="name">ir.cron.tree</field>
            <field name="inherit_id" ref="base.ir_cron_view_tree" />
            <field name="model">ir.cron</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='numbercall']" position="after">
                    <button icon="gtk-go-forward" name="action_run" string="Run Now" type="object"/>
                </xpath>
            </field>
        </record>
    </data>
</openerp>