<?xml version="1.0" encoding="UTF-8"?>

<templates id="template" xml:space="preserve">
    
	<t t-name="OrderRepaintButtonWidget">
		 <div class="order-selector">
			 <span class="order-button square wk_order_button" >All orders</span>

		</div>
	</t>

	<t t-name="PosOrderScreenWidget">
		<div  class="wk_main_body screen">
			<div class="screen-content">
				<div class="wk_header_body">
	        		<div style="padding-top:10px">
	    		 		<button class="input-button wk_cancel_button">
	    		 			<i class='fa fa-angle-double-left'></i>
	    		 			Cancel
						</button>
	            	</div>
    			</div>
				<div  class="wk_table_body">
					<table class='client-list'>
						<thead>
							<tr>
								<th>Order Ref</th>
								<th>Customer</th>
								<th>Order Date</th>

								<th>Receipt </th>
								<th>Receipt PDF</th>
							</tr>
						</thead>
						<tbody class='client-list-contents'>

						</tbody>

					</table>
				</div>
			</div>
    	</div>
    </t>

</templates>

    
